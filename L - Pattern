#include <iostream> 
using namespace std; 

int height = 5; 

int width = (2 * height) - 1; 
void printL() 
{ 
    int i, j; 
    for (i = 0; i < height; i++) { 
        cout <<"*"; 
        for (j = 0; j <= height; j++) { 
            if (i == height - 1) 
                cout <<"*"; 
            else
                cout <<" "; 
        } 
        cout <<"\n"; 
    } 
} 
int main()
{
printL();
return 0;
}
