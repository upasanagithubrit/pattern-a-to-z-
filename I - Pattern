#include <iostream> 
using namespace std; 

int height = 5; 

int width = (2 * height) - 1; 

void printI() 
{ 
    int i, j; 
    for (i = 0; i < height; i++) { 
        for (j = 0; j < height; j++) { 
            if (i == 0 || i == height - 1) 
                cout <<"*"; 
            else if (j == height / 2) 
                cout <<"*"; 
            else
                cout <<" "; 
        } 
        cout <<"\n"; 
    } 
} 

int main() 
{ 
   
printI() ;
    
    return 0; 
} 
